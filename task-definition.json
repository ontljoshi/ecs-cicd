{
    "containerDefinitions": [
        {
            "name": "nodejs",
            "image": "dipendrac3/cicd-test:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "nodejs-9005-tcp",
                    "containerPort": 9005,
                    "hostPort": 9005,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/nodejs-td",
                    "awslogs-create-group": "true",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "nodejs-td",
    "taskRoleArn": "arn:aws:iam::058264422797:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::058264422797:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "revision": 2,
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072"
}